<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gj.mapper.UserMapper">
    <select id="findOne" resultType="UserMessage" parameterType="String">
        select * from managerial.usermessage where username = #{username} and password = #{password};
    </select>
</mapper>